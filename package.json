	},
	"devDependencies": {
		"@astrojs/mdx": "4.0.8",
		"@astrojs/sitemap": "3.2.1",
		"@astrojs/tailwind": "6.0.0",
		"@pagefind/default-ui": "^1.0.4",
		"@playform/compress": "^0.1.7",
		"@tailwindcss/typography": "0.5.10",
		"@types/mdast": "^4.0.4",
